{
  "kind": "collectionType",
  "collectionName": "chef_profiles",
  "info": {
    "singularName": "chef-profile",
    "pluralName": "chef-profiles",
    "displayName": "Chef Profile",
    "description": "Chef profile information and details"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "bio": {
      "type": "text",
      "required": true
    },
    "specialties": {
      "type": "json",
      "default": []
    },
    "cuisineTypes": {
      "type": "json",
      "default": []
    },
    "yearsOfExperience": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "hourlyRate": {
      "type": "decimal",
      "required": true,
      "min": 0
    },
    "profilePhoto": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "portfolioImages": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    },
    "certifications": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images", "files"]
    },
    "averageRating": {
      "type": "decimal",
      "default": 0,
      "min": 0,
      "max": 5
    },
    "totalBookings": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "location": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string"
    },
    "governorate": {
      "type": "string"
    },
    "isAvailable": {
      "type": "boolean",
      "default": true
    },
    "isApproved": {
      "type": "boolean",
      "default": false
    },
    "bookings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::booking.booking",
      "mappedBy": "chef"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "chef"
    }
  }
}

