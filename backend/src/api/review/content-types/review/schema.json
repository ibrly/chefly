{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review",
    "description": "Chef reviews and ratings from clients"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "booking": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::booking.booking",
      "inversedBy": "review"
    },
    "client": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "chef": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::chef-profile.chef-profile",
      "inversedBy": "reviews"
    },
    "rating": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 5
    },
    "comment": {
      "type": "text"
    },
    "isApproved": {
      "type": "boolean",
      "default": true
    }
  }
}

